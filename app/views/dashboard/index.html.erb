<h1>Hello <%= current_user.email %></h1>
<%= link_to "Log Out", destroy_user_session_path, data: { "turbo-method": :delete }, class: "btn btn-primary" %>

<%= form_with model: Tweet.new, url: tweets_path, local: true do |f| %>
  <%= f.text_field :body %>
  <%= f.submit %>
<% end %>

<% @tweet_bodies.each do |text| %>
  <ul>
    <li>
      <%= text %>
    </li>
  </ul>
<% end %>